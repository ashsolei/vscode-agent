    "configuration": {
      "title": "VS Code Agent",
      "properties": {
        "vscodeAgent.defaultProfile": {
          "type": "string",
          "default": "",
          "description": "Default profile to activate on startup (e.g. 'frontend', 'backend', 'fullstack')"
        },
        "vscodeAgent.locale": {
          "type": "string",
          "default": "auto",
          "enum": ["auto", "en", "sv"],
          "enumDescriptions": [
            "Auto-detect from VS Code language",
            "English",
            "Svenska"
          ],
          "description": "Language for agent messages"
        },
        "vscodeAgent.cache.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable response caching for repeated prompts"
        },
        "vscodeAgent.cache.ttlMinutes": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "maximum": 1440,
          "description": "Cache time-to-live in minutes"
        },
        "vscodeAgent.cache.maxEntries": {
          "type": "number",
          "default": 200,
          "minimum": 10,
          "maximum": 5000,
          "description": "Maximum number of cached responses"
        },
        "vscodeAgent.telemetry.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable local telemetry collection (never sent externally)"
        },
        "vscodeAgent.guardrails.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable guardrails and rollback protection"
        },
        "vscodeAgent.guardrails.dryRun": {
          "type": "boolean",
          "default": false,
          "description": "Run autonomous agents in dry-run mode (preview only)"
        },
        "vscodeAgent.memory.maxEntries": {
          "type": "number",
          "default": 500,
          "minimum": 10,
          "maximum": 10000,
          "description": "Maximum number of agent memories to persist"
        },
        "vscodeAgent.memory.pruneAfterDays": {
          "type": "number",
          "default": 30,
          "minimum": 1,
          "maximum": 365,
          "description": "Auto-prune memories older than this many days"
        },
        "vscodeAgent.codeLens.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Show Agent CodeLens in source files"
        },
        "vscodeAgent.notifications.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Show notification center alerts for agent events"
        },
        "vscodeAgent.models.default": {
          "type": "string",
          "default": "auto",
          "description": "Default LLM model to use (e.g. 'gpt-4o', 'claude-3.5-sonnet', 'auto')"
        },
        "vscodeAgent.autonomous.maxSteps": {
          "type": "number",
          "default": 10,
          "minimum": 1,
          "maximum": 50,
          "description": "Max steps for autonomous agent execution"
        },
        "vscodeAgent.autonomous.confirmBeforeApply": {
          "type": "boolean",
          "default": true,
          "description": "Require confirmation before applying autonomous changes"
        },
        "vscodeAgent.sidebar.showOnStartup": {
          "type": "boolean",
          "default": false,
          "description": "Auto-show Agent Explorer sidebar on startup"
        },
        "vscodeAgent.rateLimitPerMinute": {
          "type": "number",
          "default": 30,
          "minimum": 5,
          "maximum": 200,
          "description": "Maximum agent invocations per minute (rate limiting)"
        }
      }
    },
